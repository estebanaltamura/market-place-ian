{
  "version": 2,
  "builds": [
    {
      "src": "api/scrape.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/scrape",
      "dest": "/api/scrape.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
